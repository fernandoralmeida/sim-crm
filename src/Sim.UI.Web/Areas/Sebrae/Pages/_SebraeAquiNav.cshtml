    <a asp-page="/Index" asp-route-Src="@Model">Não
      Lançados</a>
  </li>
  <li class="@SebraeAquiNavPages.LancadosNavClass(ViewContext) submit">
    <a asp-page="/Rae/Lancados" asp-route-Src="@Model">Lançados</a>
  </li>
